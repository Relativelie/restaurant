{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "lines-between-class-members": "off",
    "object-curly-newline": "off",
    "array-callback-return": "off"
  }
}
